            "enabled",
            "only_band",
            "router",
            "supported_keys"
          ],
          "properties": {
            "enabled": {
              "type": "boolean"
            },
            "only_band": {
              "type": "boolean"
            },
            "router": {
              "$ref": "#/definitions/Contract"
            },
            "supported_keys": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
        },
        "Contract": {
          "type": "object",
          "required": [
            "address",
            "code_hash"
          ],
          "properties": {
            "address": {
              "$ref": "#/definitions/Addr"
            },
            "code_hash": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      }
    },
